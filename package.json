{
    "name": "@reolink/web.basic-component",
    "version": "0.1.25",
    "description": "reolink basic-component",
    "main": "dist/index.js",
    "scripts": {
        "build": "npm run clean && cross-env REO_ENV=production webpack --config build/webpack.prod.config.js",
        "rebuild": "npm run clean && npm run lint && npm run build",
        "test": "echo See directory sources/tests",
        "clean": "rm -rf dist",
        "lint": "eslint --ext ts --ext tsx src",
        "dev": "webpack serve --config build/webpack.dev.config.js",
        "dev:dashboard": "webpack-dashboard -- webpack-dev-server --config build/webpack.dev.config.js",
        "dev-document": "rm -rf _site && cross-env NODE_ENV=development reolink-bisheng-start -c ./reolink-doc.config.js",
        "build-document": "rm -rf _site && cross-env NODE_ENV=production reolink-bisheng-build -c ./reolink-doc.config.js -m src/basic"
    },
    "keywords": [
        "npm",
        "basic-component"
    ],
    "author": "akane-wangqian<wangqian@sz-bcs.com.cn>",
    "repository": {
        "type": "git",
        "url": "git+https://git.develop.reolink.com.cn/wangqian/basic-component.git"
    },
    "bugs": {
